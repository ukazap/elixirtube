video:
  title: <%= inspect(@title) %>
  raw_title: <%= inspect(@raw_title) %>
  description: <%= inspect(@description) %>
  speaker_names:<%= if Enum.empty?(@speaker_names) do %> []<% else %><%= for name <- @speaker_names do %>
    - <%= name %><% end %><% end %>
  source: <%= @source %>
  thumbnails:
    xs: <%= @thumbnails.xs %>
    s: <%= @thumbnails.s %>
    m: <%= @thumbnails.m %>
    l: <%= @thumbnails.l %>
    xl: <%= @thumbnails.xl %>
  published_at: <%= @published_at %>